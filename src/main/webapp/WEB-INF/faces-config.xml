<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <description>Pageflow config</description>

        <navigation-case>
            <from-outcome>signup</from-outcome>
            <to-view-id>/signup.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/dashboard.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>edit_organization</from-outcome>
            <to-view-id>/edit_organization.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>edit_template</from-outcome>
            <to-view-id>/edit_template.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
